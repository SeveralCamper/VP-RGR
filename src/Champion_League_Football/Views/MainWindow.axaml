<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:Champion_League_Football.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Champion_League_Football.Views.MainWindow"
        Icon="/Assets/avalonia-logo.ico"
        Title="Champion_League_Football">

    <Design.DataContext>
        <vm:MainWindowViewModel/>
    </Design.DataContext>

	<StackPanel>
		<TextBox Name="TextBox1" Text="{Binding Text1}"/>
	    <TextBox Text="{Binding #TextBox1.Text}"/>
	    <Button Content="Finish" Click="HandlerOnClick"/>
	</StackPanel>
	
</Window>
